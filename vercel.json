{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.php",
      "use": "@vercel/php"
    },
    {
      "src": "wp/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/wp-admin/(.*)",
      "dest": "/wp/wp-admin/$1"
    },
    {
      "src": "/wp-content/(.*)",
      "dest": "/wp/wp-content/$1"
    },
    {
      "src": "/wp-includes/(.*)",
      "dest": "/wp/wp-includes/$1"
    },
    {
      "src": "/(.*)\\.php",
      "dest": "/api/$1.php"
    },
    {
      "src": "/",
      "dest": "/api/index.php"
    }
  ],
